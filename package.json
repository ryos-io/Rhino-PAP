{
  "name": "rhino-ui",
  "version": "0.0.1",
  "private": true,
  "description": "Frontend of Rhino Load Testing Platform",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "webpack-dev-server --client-log-level debug --mode development --hot --progress --color --port 3000 --host 0.0.0.0 --open",
    "build": "webpack --mode production --progress --colors --env.TSSTRICT",
    "tslint": "tslint 'src/**/*.{ts,tsx}'",
    "tslint:fix": "npm run tslint -- --fix",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}' './__test__/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.14.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-actions": "2.3.1",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.4.35",
    "@types/webpack-env": "1.13.6",
    "acorn": "^6.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.0",
    "css-modules-typescript-loader": "^1.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-beta.7",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.17",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.6",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.3",
    "url-loader": "^1.1.2",
    "util": "^0.11.1",
    "webpack": "^4.35.3",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.1",
    "reactn": "^2.1.4",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.4",
    "redux-logger": "^3.0.6",
    "redux-starter-kit": "^0.4.3",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "(cd ../ && git status --short) | grep -qoE '.(js|ts)x?' && npm run prettier || true",
      "pre-push": "(cd ../ && git merge-base --fork-point origin/master HEAD) | grep -qoE '.(js|ts)x?' && npm run build && npm run test && npm run tslint || true"
    }
  }
}
